{
  "name": "w3lib",
  "version": "2.1.3",
  "description": "TypeScript API for Warcraft III custom maps.",
  "author": "Halithor",
  "contributors": [
    "Jaron Hamlik <jdhamlik@gmail.com>"
  ],
  "license": "MIT",
  "main": "./dist/index",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.lua",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "tstl",
    "check": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm.cmd run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@typescript-to-lua/language-extensions": "^1.19.0",
    "eslint": "^8.54.0",
    "lua-types": "^2.13.1",
    "prettier": "^3.1.0",
    "typescript": "~5.2.2",
    "typescript-to-lua": "^1.22.0",
    "war3-types": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Halithor/w3lib.git"
  },
  "bugs": {
    "url": "https://github.com/Halithor/w3lib/issues"
  },
  "homepage": "https://github.com/Halithor/w3lib#readme"
}
